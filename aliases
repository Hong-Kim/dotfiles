# Unix
alias ln='ln -v'
alias mkdir='mkdir -p'
alias ...='../..'
alias lh='ls -Alh'

# Tmux
alias ta="tmux attach -t"
alias tn="tmux new -s"
alias tls="tmux ls"

# Mac
alias dwn="cd ~/Downloads"
alias dv="cd ~/dev"

# Dev
alias gph="git push heroku master"
alias fbd="npm run build; firebase deploy"
